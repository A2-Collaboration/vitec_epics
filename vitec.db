#  @param PREFIX something like SYS:SUBSYS ("CB:CB:ADC_1A")
#

record (ai, "$(PREFIX):VITEC:VP5")
{
	field(DTYP, "stream")
	field(INP, "@$(PROTO) read_VP5 VITEC")
	field(SCAN, "I/O Intr")
	field(DESC, "+5 Volt supply of VITEC")
	field(LINR, "LINEAR")
	field(EGU, "V")
	field(LOPR, 4)
	field(HOPR, 6)
	field(LOW,  4.8)
	field(HIGH, 5.2)
	field(LOLO, 4.5)
	field(HIHI, 5.5)
	field(LSV,  "MINOR")
	field(LLSV, "MAJOR")
	field(HSV,  "MINOR")
	field(HHSV, "MAJOR")
}

